<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Grpc.AspNetCore" Version="2.32.0" />
  </ItemGroup>

  <ItemGroup>
    <Protobuf Include="..\protos\chat-service.proto" GrpcServices="Server">
      <Link>Protos\chat-service.proto</Link>
    </Protobuf>
    <Protobuf Include="..\protos\message.proto">
      <Link>Protos\message.proto</Link>
    </Protobuf>
  </ItemGroup>

</Project>
